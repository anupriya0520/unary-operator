class unary{
     public static void main(String[] args){
           int a=10;
           ++a;//11
           int b=a++; //b=11 ->a=a+1=12
           System.out.println(b);//11
           int c=--b+a++ -b;//10+12-10=12 ->a=13->b=10
           int d= ++a - ++b;//14-11=3
           System.out.println(c);//12
           System.out.println(d);//3
      }
}
